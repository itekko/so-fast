<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sofast.system.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sofast.system.entity.Log">
    <result column="id" property="id" />
    <result column="versions" property="versions" />
    <result column="description" property="description" />
    <result column="delete_flag" property="deleteFlag" />
    <result column="create_by" property="createBy" />
    <result column="create_time" property="createTime" />
    <result column="update_by" property="updateBy" />
    <result column="update_time" property="updateTime" />
        <result column="user_id" property="userId" />
        <result column="operation" property="operation" />
        <result column="times" property="times" />
        <result column="method" property="method" />
        <result column="url" property="url" />
        <result column="params" property="params" />
        <result column="ip_address" property="ipAddress" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        versions,
        description,
        delete_flag,
        create_by,
        create_time,
        update_by,
        update_time,
        user_id, operation, times, method, url, params, ip_address
    </sql>

</mapper>
